<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="PersistenceUnit" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>persistence.component.Component</class>
    <class>persistence.component.attribute.AttributeSpecification</class>
    <class>persistence.component.holder.ComponentHolder</class>
    <class>persistence.component.method.Method</class>
    <class>persistence.dataset.DatasetDefinition</class>
    <class>persistence.dataset.attribute.AttributeDefinition</class>
    <class>persistence.flow.Flow</class>
    <class>persistence.flow.edge.Edge</class>
    <class>persistence.flow.node.Node</class>
    <class>persistence.parameter.ParameterDefinition</class>
    <class>persistence.parameter.ParameterValue</class>
    <class>persistence.registry.RegistryCategory</class>
    <class>persistence.registry.RegistryItem</class>
    <class>persistence.senario.Senario</class>
    <class>persistence.senario.destination.Destination</class>
    <class>persistence.senario.source.Source</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:sqlite:database/main.db"/>
      <property name="javax.persistence.jdbc.password" value=""/>
      <property name="javax.persistence.jdbc.driver" value="org.sqlite.JDBC"/>
      <property name="javax.persistence.jdbc.user" value=""/>
      <property name="eclipselink.ddl-generation" value="create-tables"/>
      <property name="eclipselink.logging.level" value="OFF"/>
    </properties>
  </persistence-unit>
</persistence>
